local function toggleFunc(curr)
	if curr then
		local find = table.find(toggledFeatures,curr)
		if find then
			callbacks[curr](false)
			table.remove(toggledFeatures,find)
			return
		elseif not find then
			table.insert(toggledFeatures,curr)
			callbacks[curr](true)
		end
	end
end

